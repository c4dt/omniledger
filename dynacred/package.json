{
  "name": "@c4dt/dynacred",
  "version": "0.0.5",
  "description": "The library used by the omniledger-ui and the personhood.online app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/c4dt/omniledger.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "ts-node --prefer-ts-exts node_modules/.bin/jasmine",
    "prepare": "npm run lint && npm run build",
    "cover": "nyc jasmine",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project ."
  },
  "keywords": [
    "cothority",
    "network"
  ],
  "author": "C4DT",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@c4dt/cothority": "^3.1.16",
    "cross-fetch": "^3.0.4",
    "dexie": "^2.0.4",
    "tweetnacl-ts": "^1.0.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.15",
    "jasmine": "^3.4.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "require": [
      "ts-node/register"
    ]
  },
  "files": [
    "build/*"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts"
}
