{
  "name": "@c4dt/dynacred",
  "version": "0.3.2",
  "description": "The library used by the omniledger-ui and the personhood.online app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/c4dt/omniledger.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "ts-node --project tsconfig.spec.json -r tsconfig-paths/register --prefer-ts-exts node_modules/.bin/jasmine",
    "prepare": "npm run lint && npm run build",
    "cover": "nyc jasmine",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project ."
  },
  "keywords": [
    "cothority",
    "network"
  ],
  "author": "C4DT",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@c4dt/cothority": "^3.4.1-1",
    "@c4dt/kyber": "^3.4.0-1",
    "cross-fetch": "^3.0.4",
    "crypto-browserify": "^3.12.0",
    "dexie": "^2.0.4",
    "elliptic": "^6.5.2",
    "isomorphic-ws": "^4.0.1",
    "keccak": "^2.1.0",
    "long": "^4.0.0",
    "moment": "^2.24.0",
    "protobufjs": "^6.8.8",
    "pure-uuid": "^1.5.7",
    "shuffle-array": "^1.0.1",
    "sprintf-js": "^1.1.2",
    "toml": "^3.0.0",
    "tweetnacl-ts": "^1.0.3",
    "url-parse": "^1.4.7",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.2",
    "dockerode": "^3.0.1",
    "jasmine": "^3.5.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "require": [
      "ts-node/register"
    ]
  },
  "files": [
    "build/*"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts"
}
